#include <stdio.h>

int main(void) {
    int i, m, n, j; 
    m = 2, n = 2;
     int A[m][n];
    
    printf("Insira os elementos da matriz A:\n");
    for(i = 0; i < m; i++) {                  
        for(j = 0; j < n; j++) {
            scanf("%d", &A[i][j]);
        }
    }                          

    int determinante = A[0][0] * A[1][1] - A[0][1] * A[1][0];                                        

    printf("A matriz  resultante sera: %d\n", determinante);

    return 0;
}

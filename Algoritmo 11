#include <stdio.h>

int num1;
int main(void) {
  printf("Escreva um numero primo:\n");
  scanf("%i", &num1);
    if (num1 == 2 || num1 == 3 || num1 == 5){
    printf("O numero e primo");
    } else if (num1 % 2 == 0 || num1 % 3 == 0 || num1 % 5 == 0 || num1 == 1){
    printf("Nao e primo!");
    } else {
    printf("E primo");
    }
  return 0;
}

#include <stdio.h>

int num1, num2, i, total;

int main(void) {
  printf("Escreva um numero:");
  scanf("%i", &num1);
  printf("Escreva a potencia:");
  scanf("%i", &num2);
  total = 1;  
  i = 0;
  while (i < num2) {
    total = total * num1; 
    i++;
  }
  printf("O numero elevado a potencia sera: %i\n", total);
  return 0;
}
